@model StudentPortal.STU_DinhMuc

@{
    ViewBag.Title = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<div class="row-fluid">
	<div class="span12">
		<!-- BEGIN SAMPLE FORM PORTLET-->
		<div class="portlet box blue">
			<div class="portlet-title">
				<h4><i class="icon-reorder"></i>Delete</h4>
				<div class="tools">
					<a href="javascript:;" class="collapse"></a>
					<a href="#portlet-config" data-toggle="modal" class="config"></a>
					<a href="javascript:;" class="reload"></a>
					<a href="javascript:;" class="remove"></a>
				</div>
			</div>
			<div class="portlet-body form">
                <!-- BEGIN FORM-->
				@using (Html.BeginForm("DeleteConfirmed", "DinhMuc", FormMethod.Post, new { @class = "form-horizontal" }))
	{


    <div class="control-group">
            @Html.LabelFor(model => model.ID_chuc_danh, "STU_ChucDanh", new { @class = "control-label" })
		<div class="controls">
			@Html.TextBoxFor(model => model.ID_chuc_danh, new { @class = "medium m-wrap",disabled="" })
		</div>
	</div>


    <div class="control-group">
            @Html.LabelFor(model => model.ID_nhiem_vu, new { @class = "control-label" })
		<div class="controls">
			@Html.TextBoxFor(model => model.ID_nhiem_vu, new { @class = "medium m-wrap",disabled="" })
		</div>
	</div>


    <div class="control-group">
            @Html.LabelFor(model => model.So_tiet, new { @class = "control-label" })
		<div class="controls">
			@Html.TextBoxFor(model => model.So_tiet, new { @class = "medium m-wrap",disabled="" })
		</div>
	</div>

        <div class="form-actions">
            <input type="submit" class="btn red" value="Delete" />
            @Html.ActionLink("Cancel", "Index",new{},new { @class = "btn" })
        </div>
}
			</div>
		</div>
	</div>
</div>

</fieldset>
@using (Html.BeginForm()) {
    <p>
        
    </p>
}
