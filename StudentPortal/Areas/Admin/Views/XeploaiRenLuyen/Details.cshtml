@model StudentPortal.STU_XeploaiRenLuyen

@{
    ViewBag.Title = "Details";
	Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>

<div class="row-fluid">
	<div class="span12">
		<!-- BEGIN SAMPLE FORM PORTLET-->
		<div class="portlet box blue">
			<div class="portlet-title">
				<h4><i class="icon-reorder"></i>Details</h4>
				<div class="tools">
					<a href="javascript:;" class="collapse"></a>
					<a href="#portlet-config" data-toggle="modal" class="config"></a>
					<a href="javascript:;" class="reload"></a>
					<a href="javascript:;" class="remove"></a>
				</div>
			</div>
			<div class="portlet-body form">
			<form class="form-horizontal" action="#">

    <div class="control-group">
            @Html.LabelFor(model => model.Xep_loai, new { @class = "control-label" })
		<div class="controls">
			@Html.TextBoxFor(model => model.Xep_loai, new { @class = "medium m-wrap",disabled="" })
		</div>
	</div>

    <div class="control-group">
            @Html.LabelFor(model => model.Tu_diem, new { @class = "control-label" })
		<div class="controls">
			@Html.TextBoxFor(model => model.Tu_diem, new { @class = "medium m-wrap",disabled="" })
		</div>
	</div>

    <div class="control-group">
            @Html.LabelFor(model => model.Den_diem, new { @class = "control-label" })
		<div class="controls">
			@Html.TextBoxFor(model => model.Den_diem, new { @class = "medium m-wrap",disabled="" })
		</div>
	</div>

    <div class="control-group">
            @Html.LabelFor(model => model.He_so, new { @class = "control-label" })
		<div class="controls">
			@Html.TextBoxFor(model => model.He_so, new { @class = "medium m-wrap",disabled="" })
		</div>
	</div>
					<div class="form-actions">
    @Html.ActionLink("Edit", "Edit", new { id=Model.ID_xep_loai },new { @class = "btn blue" }) |
                        @Html.ActionLink("Cancel", "Index",new{},new { @class = "btn" })
					</div>
				</form>
			</div>
		</div>
	</div>
</div>

