@model StudentPortal.STU_DiemRenLuyen

@{
    ViewBag.Title = "Details";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>

<div class="row-fluid">
	<div class="span12">
		<!-- BEGIN SAMPLE FORM PORTLET-->
		<div class="portlet box blue">
			<div class="portlet-title">
				<h4><i class="icon-reorder"></i>Details</h4>
				<div class="tools">
					<a href="javascript:;" class="collapse"></a>
					<a href="#portlet-config" data-toggle="modal" class="config"></a>
					<a href="javascript:;" class="reload"></a>
					<a href="javascript:;" class="remove"></a>
				</div>
			</div>
			<div class="portlet-body form">
			<form class="form-horizontal" action="#">

    <div class="control-group">
            @Html.LabelFor(model => model.Hoc_ky, new { @class = "control-label" })
		<div class="controls">
			@Html.TextBoxFor(model => model.Hoc_ky, new { @class = "medium m-wrap",disabled="" })
		</div>
	</div>

    <div class="control-group">
            @Html.LabelFor(model => model.Nam_hoc, new { @class = "control-label" })
		<div class="controls">
			@Html.TextBoxFor(model => model.Nam_hoc, new { @class = "medium m-wrap",disabled="" })
		</div>
	</div>

    <div class="control-group">
            @Html.LabelFor(model => model.ID_sv, "STU_HoSoSinhVien", new { @class = "control-label" })
		<div class="controls">
			@Html.TextBoxFor(model => model.ID_sv, new { @class = "medium m-wrap",disabled="" })
		</div>
	</div>

    <div class="control-group">
            @Html.LabelFor(model => model.ID_loai_rl, "STU_LoaiRenLuyen", new { @class = "control-label" })
		<div class="controls">
			@Html.TextBoxFor(model => model.ID_loai_rl, new { @class = "medium m-wrap",disabled="" })
		</div>
	</div>

    <div class="control-group">
            @Html.LabelFor(model => model.Diem, new { @class = "control-label" })
		<div class="controls">
			@Html.TextBoxFor(model => model.Diem, new { @class = "medium m-wrap",disabled="" })
		</div>
	</div>
					<div class="form-actions">
    @Html.ActionLink("Edit", "Edit", new { id=Model.ID_diem_rl },new { @class = "btn blue" }) |
                        @Html.ActionLink("Cancel", "Index",new{},new { @class = "btn" })
					</div>
				</form>
			</div>
		</div>
	</div>
</div>

